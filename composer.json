{
    "name": "swis/database-extra-bundle",
    "type": "symfony-bundle",
    "description": "This bundle extends the behaviour of a SQLite database in a Symfony2 project.",
    "keywords": ["SQLite database behaviour"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sascha Weyers",
            "email": "kontakt@sw-is.de"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "~2.3",
        "doctrine/dbal": ">=2.3",
        "doctrine/orm": ">=2.3"
    },
    "require-dev": {
    },
    "suggest": {
    },
    "autoload": {
        "psr-0": { "Swis\\Bundle\\DatabaseExtraBundle": "" }
    },
    "target-dir": "Swis/Bundle/DatabaseExtraBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
